{
  "name": "swiv-map-insite",
  "version": "1.0.0-alpha.5",
  "description": "Swiv mapper service to work with Insite Software API",
  "homepage": "https://github.com/absolunet/swiv-map-insite",
  "author": {
    "name": "Absolunet",
    "url": "https://absolunet.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/absolunet/swiv-map-insite.git"
  },
  "bugs": {
    "url": "https://github.com/absolunet/swiv-map-insite/issues"
  },
  "scripts": {
    "dev": "npm run development",
    "dev-local": "npm run development-local",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development-local": "cross-env NODE_ENV=development-local node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-local": "npm run development-local -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "@absolunet/eslint-config-core": "0.3.0",
    "cross-env": "^5.1",
    "eslint-loader": "^2.0.0",
    "laravel-mix": "2.1.11",
    "laravel-mix-eslint": "^0.1.1"
  }
}
